{
  "root": true,
  "parser": "babel-eslint",
  "extends": ["standard", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-expressions": [0],
    "no-empty": [0],
    "comma-dangle": [0],
    "space-before-function-paren": [0],
    "import/no-cycle": [0],
    "max-len": [0]
  }
}
